<%- include("./_partials/head") %>

<js>
  <script defer src="/js/home/main.js"></script>
</js>

<a href="/">
  <h1 class="container mb-4">HOME</h1>
</a>

<!-- 웹소켓으로 하면 액션, 메소드 필요없어여 -->
<form url-form class="container mb-4">
  <div class="input-group">
    <input url name="url" type="text" class="form-control" placeholder="dofollow" required>
    <button class="btn btn-outline-secondary" type="submit">CHECK</button>
  </div>
</form>

<form action="/" method="post" class="container mb-4">
  <p>URL : <%= url %></p>
  <div class="input-group">
    <input name="add" type="text" class="form-control" value="<%= url %>" readonly>
    <button class="btn btn-outline-secondary" type="submit">ADD</button>
  </div>
</form>


<table class="table container">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">url</th>
      <th scope="col">date</th>
    </tr>
  </thead>
  <tbody>
    <% for (var i= 0; i< list.length; i++){ %>
    <tr>
      <th scope="row"><%= i %></th>
      <td><%= list[i].addUrl %></td>
      <td><%= list[i].date %></td>
    </tr>
    <% } %>

  </tbody>
</table>






